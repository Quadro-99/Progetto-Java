package model;

import model.ContrattoLavoroEnum.ContrattoLavoro;

public class Segretario extends Impiegato {

	public Segretario() { }

	public Segretario(String nome, String cognome, double stipendio, ContrattoLavoro contratto, String sportello) {
		super(nome, cognome, stipendio, contratto);
		this.sportello = sportello;
	}

	public String getSportello() {
		return sportello;
	}


	public void setSportello(String sportello) {
		this.sportello = sportello;
	}


	private String sportello;
}
